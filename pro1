function setup() {
  createCanvas(windowWidth,windowHeight);
  rectMode(CENTER)
}
function draw() {
  background(0);
  if(mouseX>=width/2-50 && mouseX<=width/2+50){
    fill(0,0,255,100);
  } else {
    fill(255,0,0,100);
  }
  rect(width/2,height/2,100,100);
  ellipse(width/2,height/2,100,30); 
  ellipse(width/2,height/2,30,100);
  if(mouseIsPressed){
    background(255);
    rect(width/2,height/2,100,100);
    ellipse(width/2,height/2,100,30); 
    ellipse(width/2,height/2,30,100);
  }
}
